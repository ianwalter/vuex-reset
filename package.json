{
  "name": "@ianwalter/vuex-reset",
  "version": "2.0.3",
  "description": "A Vuex plugin that makes restoring initial state to the store simple",
  "main": "dist/vuex-reset.js",
  "module": "dist/vuex-reset.m.js",
  "iife": "dist/vuex-reset.iife.js",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "eslint index.js test.js site --ext js,vue",
    "dist": "dist --babel --no-iife",
    "iife": "dist --name VuexReset --babel --inline --no-cjs --no-esm",
    "pretest": "npm run dist && npm run iife",
    "test": "ava",
    "build:site": "webpack --env.singleRun"
  },
  "keywords": [
    "vue",
    "vuex",
    "restore",
    "reset",
    "initial",
    "state"
  ],
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianwalter/vuex-reset.git"
  },
  "bugs": {
    "url": "https://github.com/ianwalter/vuex-reset/issues"
  },
  "homepage": "https://github.com/ianwalter/vuex-reset#readme",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@ianwalter/clone": "^2.0.1",
    "@ianwalter/merge": "^2.0.1"
  },
  "devDependencies": {
    "@fullhuman/purgecss-loader": "^1.0.0",
    "@ianwalter/babel-preset-lib": "^1.0.3",
    "@ianwalter/dist": "^1.5.1",
    "@ianwalter/dracula": "^2.0.0",
    "@ianwalter/prism-loader": "^1.2.0",
    "ava": "^1.2.0",
    "babel-loader": "^8.0.5",
    "bootstrap": "^4.2.1",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "np": "^3.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "style-loader": "^0.23.1",
    "vue": "^2.5.22",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.5.22",
    "vuex": "^3.1.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.10"
  },
  "browserslist": [
    "> 0.5%"
  ],
  "babel": {
    "presets": [
      "@ianwalter/lib"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "standard",
      "plugin:vue/recommended"
    ],
    "plugins": [
      "vue"
    ],
    "rules": {
      "vue/multiline-html-element-content-newline": 0
    }
  },
  "eslintIgnore": [
    "dist"
  ]
}
